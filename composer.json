{
	"require": {
		"npm-asset/css-ui-simple": "^2.4",
		"npm-asset/open-sans-fontface": "^1.4"
	},
	"config": {
		"fxp-asset": {
			"ignore-files": {
				"npm-asset/css-ui-simple": [
					"src/css/"
				],
				"npm-asset/open-sans-fontface": [
					"sass",
					"*.html",
					"*.less",
					"*.scss"
				]
			},
			"installer-paths": {
				"npm-asset-library": "www/assets"
			}
		}
	}
}
