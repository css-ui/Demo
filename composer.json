{
	"require": {
		"npm-asset/css-ui-responsive-menu": "^3.0.8"
	},
	"config": {
		"fxp-asset": {
			"ignore-files": {
				"npm-asset/normalize.css": [
					"*.md"
				],
				"npm-asset/css-ui-simple": [
					"css/src/",
					"*.md"
				],
				"npm-asset/css-ui-responsive-menu": [
					"src/*.html",
					"*.md"
				],
				"npm-asset/open-sans-fontface": [
					"sass",
					"*.html",
					"*.less",
					"*.scss",
					"*.md"
				],
				"npm-asset/jquery": [
					"src",
					"external",
					"*.txt",
					"*.md"
				],
				"npm-asset/font-awesome": [
					".*",
					"*.txt",
					"less",
					"scss",
					"*.md"
				],
				"npm-asset/perfect-scrollbar": [
					"src",
					"types",
					"*.md"
				]
			},
			"installer-paths": {
				"npm-asset-library": "www/assets"
			}
		}
	}
}
